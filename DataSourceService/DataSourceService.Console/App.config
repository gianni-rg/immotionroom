<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
    
  <appSettings>
    <add key="InstanceID" value="DS_Center" />
    <add key="IsFake" value="false" /><!-- TEMPORARY - ONLY FOR DEBUG -->
    <!--<add key="SavedSessionId" value="20160108_104627" />--><!-- TEMPORARY - ONLY FOR DEBUG -->
    <!--<add key="SavedSessionPath" value="E:\TBoxRecordedSessionConv" />--><!-- TEMPORARY - ONLY FOR DEBUG -->
    <!--<add key="DataStreamerEndpoint" value="127.0.0.1"/>-->
    <!--<add key="DataStreamerPort" value="5555"/>-->
    <!--<add key="ControlApiEndpoint" value="127.0.0.1"/>-->
    <!--<add key="ControlApiPort" value="9091"/>-->
    <!--<add key="AutoDiscoveryMulticastAddress" value="224.242.42.0"/>--><!-- Use 224.0.1.0 to 239.255.255.255 -->
    <!--<add key="AutoDiscoveryMulticastPort" value="18500"/>-->
    <!--<add key="AutoDiscoveryLocalPort" value="18502"/>-->
    <!--<add key="AutoDiscoveryListenerIntervalInSeconds" value="1"/>-->
    <add key="AutoDiscoveryPollingIntervalInSeconds" value="5" /><!-- 10 -->
    <!--<add key="AutoDiscoveryReachableTimeoutInSeconds" value="5"/>-->
    <!--<add key="AutoDiscoveryLoopbackLogEnabled" value="true"/>-->
    <add key="AutoDiscoveryDurationInSeconds" value="20" /><!-- 60 -->
    <!--<add key="TrackingServiceMonitorIntervalInSeconds" value="10"/>-->
    <!--<add key="AutoDiscoveryRepeatIntervalInSeconds" value="60"/>-->
    <!--<add key="AutoDiscoveryUdpLocalClientTimeoutInSeconds" value="5"/>-->
    <add key="DataRecorderSessionPath" value="E:\TBoxRecordedSessions" />
    <add key="SkeletonDataRecorderEnabled" value="true" /><!-- false -->
    <add key="ColorStreamRecorderEnabled" value="true" /><!-- false -->
    <!--<add key="ColorStreamRecorderFPS" value="30"/>-->
    <!--<add key="ColorStreamRecorderWidth" value="1920"/>--><!-- 640 -->
    <!--<add key="ColorStreamRecorderHeight" value="1080"/>--><!-- 480-->
    <!--<add key="ClientListenerTimeoutInMilliseconds" value="1000"/>-->
    <!--<add key="ClientListenerStartMaxRetries" value="3"/>-->
    <!--<add key="ClientListenerStartRetryIntervalInMilliseconds" value="2000"/>-->
    <add key="DepthStreamRecorderEnabled" value="false" />
    <!--<add key="DepthStreamRecorderFPS" value="30"/>-->
    <add key="DepthStreamRecorderWidth" value="512" />
    <add key="DepthStreamRecorderHeight" value="424" />
    <!--<add key="ReceivedCommandsCleanerIntervalInMinutes" value="1" />-->
    <!--<add key="MaxMessageAliveTimeInSeconds" value="15" />-->
    <!--<add key="ReceivedCommandsPollingIntervalInMilliseconds" value="1000" />-->
    <!--<add key="SystemRebootDelayInMilliseconds" value="5000" />-->
    <!--<add key="AutoDiscoveryDelayInMilliseconds" value="5000" />-->
    <!--<add key="GetLocalIpRetries" value="3" />-->
    <!--<add key="GetLocalIpIntervalInSeconds" value="60" />-->
  </appSettings>
  
   <log4net debug="false">
    <appender name="ColoredConsoleAppender" type="log4net.Appender.ColoredConsoleAppender,log4net">
      <mapping>
        <level value="INFO" />
        <foreColor value="White, HighIntensity" />
      </mapping>
      <mapping>
        <level value="DEBUG" />
        <foreColor value="White, HighIntensity" />
        <backColor value="Blue" />
      </mapping>
      <mapping>
        <level value="WARN" />
        <foreColor value="Yellow, HighIntensity" />
      </mapping>
      <mapping>
        <level value="ERROR" />
        <foreColor value="Red, HighIntensity" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout,log4net">
        <conversionPattern value="%utcdate|%m%n" />
      </layout>
    </appender>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender, log4net">
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%utcdate|[%3t]|%-5p|%c|%m%n" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender, log4net">
      <param name="File" value="Logs\DataSourceService" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="Composite" />
      <param name="DatePattern" value="'_'yyyyMMdd'.txt'" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="5MB" />
      <param name="StaticLogFileName" value="false" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%utcdate|[%3t]|%-5p|%c|%m%n" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="ColoredConsoleAppender" />
      <appender-ref ref="RollingFileAppender" />
      <!--<appender-ref ref="TraceAppender" />-->
    </root>
  </log4net>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Owin" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.1.0" newVersion="3.0.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
